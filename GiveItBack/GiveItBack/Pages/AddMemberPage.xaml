<UserControl x:Class="GiveItBack.Pages.AddMemberPage"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    mc:Ignorable="d"
    FontFamily="{StaticResource PhoneFontFamilyNormal}"
    FontSize="{StaticResource PhoneFontSizeNormal}"
    Foreground="{StaticResource PhoneForegroundBrush}"
    d:DesignHeight="480" d:DesignWidth="480">

    <Grid x:Name="LayoutRoot" Background="{StaticResource PhoneChromeBrush}">
        <CheckBox Content="Wybierz z kontaktów" VerticalAlignment="Top" IsChecked="True"/>
        <TextBox Height="72" Margin="0,77,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top"/>
        <Grid Margin="0,154,0,0"/>
    </Grid>
    
</UserControl>