        <Button Content="Wyczyść" HorizontalAlignment="Left" Margin="10,0,0,4" Width="174" BorderBrush="Black" Foreground="Black" Height="72" VerticalAlignment="Bottom" Click="Button_Click"/>
    </Grid>
</UserControl>