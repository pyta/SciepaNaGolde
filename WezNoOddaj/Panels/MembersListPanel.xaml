    mc:Ignorable="d"
    FontFamily="{StaticResource PhoneFontFamilyNormal}"
    FontSize="{StaticResource PhoneFontSizeNormal}"
    Foreground="{StaticResource PhoneForegroundBrush}" Height="407" Width="375">

    <Grid x:Name="LayoutRoot">
        <ListBox x:Name="listMembers" Margin="10,42,10,81"/>
